Order ID, Email, DTU CWIS, DTU Initials, DTU Institute, Order date, ISSN, Journal title, Article title
<% @orders.each do |order| -%>
<% if order.user && order.user.dtu? -%>
<%= order.id %>, <%= order.user.email %>, <%= order.user.user_data["dtu"]["matrikel_id"] %>, <%= order.user.user_data["dtu"]["initials"] %>, <%= t "toshokan.orders.facets.org_unit.values.#{order.org_unit}" %>, <%= order.created_at %>, "<%= order.document['issn_ss'].first %>", <%= order.document['journal_title_ts'].first.try :gsub, /[;,']/, ' ' %>, <%= order.document['title_ts'].first.try :gsub, /[;,']/, ' ' %>
<% else %>
<%= order.id %>, <%= order.email %>, , , , <%= order.created_at %>, "<%= order.document['issn_ss'].first %>", <%= order.document['journal_title_ts'].first.try :gsub, /[;,']/, ' ' %>, <%= order.document['title_ts'].first.try :gsub, /[;,']/, ' ' %>
<% end -%>
<% end -%>
