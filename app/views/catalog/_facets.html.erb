<% # main container for facets/limits menu -%>
<div id="facets" class="facets sidenav">

  <div class="facets-collapse">
  <%= render_facet_partials %>
  <% if can? :tag, Bookmark %>
    <hr/>
    <%= render_tag_partials %>
  <% end %>
  <% if has_search_parameters? %>
    <hr>
    <%= render 'cant_find_links' %>
  <% end %>
  </div>
</div>
