<div id="header-navbar-fixed-top" class="navbar navbar-fixed-top">
  <div class="navbar-inner level1-navbar">
    <div class="container">
      <div class="row">
        <div class="span6">
          <div class="pull-left">
            <h5><%= link_to t('toshokan.header.dtu'), 'http://www.dtu.dk' %> &bull; <%= link_to t('toshokan.header.dtulibrary'), root_path %></h5>
          </div>
        </div>
        <div class="span6">
          <div class="pull-right">
            <%= render :partial=>'/user_util_links' %>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="navbar-inner level2-navbar">
    <div class="container">
      <div class="row logos">
        <div class="span6">
          <%= link_to content_tag('div', '', :class => 'dtu-library-logo').html_safe, root_path, :class => 'logo pull-left' %>
        </div>
        <div class="span6">
          <%= link_to content_tag('div', '', :class => 'dtu-logo').html_safe, 'http://www.dtu.dk', :class => 'logo pull-right' %>
        </div>
      </div>
      <div class="row">
        <div class="span9 offset3">
          <div id="searchbar">         
            <%= render_search_bar %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="feedback">
  <a href=""><%= t 'toshokan.feedback' %></a>
</div>
